{
  "name": "theminimal",
  "version": "1.0.0",
  "description": "theminimal theme packages",
  "main": "js/main.js",
  "watch": {
    "patterns": [
      "src"
    ],
    "extensions": "js,jsx,scss"
  },
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "dev": "cross-env NODE_ENV=production webpack --watch --mode development --progress",
    "clean": "rm -rf build/*",
    "watch": "npm-watch"
  },
  "keywords": [
    "wordpress",
    "themes"
  ],
  "author": "Alessandro Mondelli",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cssnano": "^5.0.16",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.6",
    "sass-loader": "^12.4.0",
    "sass-mq": "^6.0.0",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "npm-watch": "^0.11.0"
  }
}
